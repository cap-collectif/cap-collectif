        "behat/mink-browserkit-driver": "^2.1",
        "behat/mink-selenium2-driver": "^1.6",
        "bex/behat-step-time-logger": "^2.0",
        "coduo/php-matcher": "^6.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "dg/bypass-finals": "^1.1",
        "friends-of-phpspec/phpspec-code-coverage": "^6.0",
        "friendsofphp/php-cs-fixer": "^3",
        "icanhazstring/composer-unused": "^0.8.9",
        "mikey179/vfsstream": "^1.6",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpspec/phpspec": "^7.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-symfony": "^1.3",
        "phpunit/phpunit": "^9.0",
        "pyrech/composer-changelogs": "^1.5",
        "rector/rector": "^1.2.0",
        "sensiolabs/behat-page-object-extension": "^2.3",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.39",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "config": {
        "allow-plugins": {
            "pyrech/composer-changelogs": true,
            "simplesamlphp/composer-module-installer": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        },
        "bin-dir": "bin",
        "platform": {
            "php": "8.1"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "exclude-from-classmap": [
            "**/Behat/"
        ],
        "psr-4": {
            "Capco\\": "src/",
            "Capco\\AppBundle\\": "src/Capco/AppBundle",
            "Capco\\AdminBundle\\": "src/Capco/AdminBundle",
            "Capco\\UserBundle\\": "src/Capco/UserBundle",
            "Capco\\AppBundle\\GraphQL\\__GENERATED__\\": "src/Capco/AppBundle/GraphQL/__generated__",
            "Capco\\Tests\\": "tests/"
        },
        "files": [
            "src/Kernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\": "spec/",
            "Capco\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-php54": "*",
        "symfony/polyfill-php55": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-apcu": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    }
}

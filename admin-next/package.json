{
  "name": "admin-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "SYMFONY_REDIS_PREFIX=dev next dev -p 3000",
    "build": "next build",
    "start": "next start -p ${ADMIN_NEXT_PORT:=3000}",
    "local-build-and-serve": "SYMFONY_ENV=test next build && SYMFONY_ENV=test next start -p ${ADMIN_NEXT_PORT:=3000}",
    "lint": "eslint ./ --ext .ts --ext .tsx",
    "prettier": "prettier --write {pages,utils,.}/*.{ts,tsx,js}",
    "test": "jest -c jest.config.js",
    "test:ci": "jest -c jest.config.js --maxWorkers=2",
    "ts": "tsc --project tsconfig.json --noEmit",
    "relay": "relay-compiler",
    "relay:watch": "relay-compiler --src ./ --schema ../schema.internal.graphql --artifactDirectory ./__generated__ --watch"
  },
  "dependencies": {
    "@babel/preset-react": "^7.14.5",
    "cookie": "^0.4.1",
    "framer-motion": "^11.0.10",
    "graphql-tag": "^2.12.6",
    "next": "14.2.25",
    "next-client-cookies": "^1.1.1",
    "nookies": "^2.5.2",
    "nuqs": "^2.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-qr-code": "^2.0.11",
    "react-relay": "^18.2.0",
    "redis": "^4.0.0",
    "relay-compiler": "^18.2.0",
    "relay-runtime": "^18.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/cookie": "^0.4.1",
    "@types/html-truncate": "^1.2.2",
    "@types/jest": "^27.0.1",
    "@types/papaparse": "^5.3.2",
    "@types/react": "^18.3.0",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^18.3.0",
    "@types/react-relay": "^18.2.0",
    "@types/relay-runtime": "18.2.5",
    "@types/styled-components": "^5.1.15",
    "chromatic": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "local-ssl-proxy": "1.3.0",
    "relay-compiler-language-typescript": "^15.0.0",
    "typescript": "^5.8.3"
  }
}

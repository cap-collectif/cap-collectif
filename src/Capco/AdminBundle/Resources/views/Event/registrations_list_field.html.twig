{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    <div>
        {% if object.isGuestListEnabled == false %}
            <span>{{ 'registrations-desabled' | trans({}, 'CapcoAppBundle') }}</span>
        {% else %}
            <span>{{ object.registrations.count }}</span>
        {% endif %}
    </div>
{% endblock %}

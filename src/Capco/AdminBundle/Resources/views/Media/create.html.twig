<form
    role="form"
    action="/admin/capco/app/media/create"
    enctype="multipart/form-data"
    method="POST"
>
    <div class="box box-primary">
        <div class="box-body">
            {% if errors %}
                <div class="alert alert-danger">
                    {{ errors }}
                </div>
            {% endif %}
            <label
                for="binaryContent"
                class="required">
                {{ 'source.type.file'|trans({}, 'CapcoAppBundle') }}
            </label>
            <input
                type="file"
                id="binaryContent"
                name="binaryContent"
                required="required"
            >
        </div>
    </div>

    <div>
        <button type="submit" class="btn btn-success" name="btn_create">
            <i class="fa fa-plus-circle" aria-hidden="true"></i>
            {{ 'btn_create'|trans({}, 'SonataAdminBundle') }}
        </button>
    </div>
</form>
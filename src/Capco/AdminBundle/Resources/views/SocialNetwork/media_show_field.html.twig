{% extends '@SonataAdmin/CRUD/base_show_field.html.twig' %}

{% block field %}
    {%  if value is not null %}
        <img
            title={{ object.title }}
            alt={{ object.title }}
            class="social__icon"
            src={{ media_public_url(object.media, 'default_socialIcon') }}
        />
    {%  endif %}
{% endblock %}

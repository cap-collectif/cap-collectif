{% extends '@CapcoAdmin/CRUD/edit.html.twig' %}

{% block title %}
    {% if admin.id(object) is not null %}
        {{ "global.edit.title"|trans({'name': admin.toString(object)|truncate(180) }, 'SonataAdminBundle') }}
    {% else %}
        {{ 'global.create'|trans({}, 'SonataAdminBundle') }}
    {% endif %}
{% endblock %}

{% block form %}
    {% include '@CapcoApp/redux_store.html.twig' with {} %}
    {{ react_render_component('GroupAdminPageApp', {
        'props': {
            'groupId': admin.subject.id,
        },
        'rendering': 'client_side'
    }) }}
{% endblock %}

{% set page_class = "cookies" %}
{% extends "@CapcoApp/base.html.twig" %}

{% block canonical_link %}{{ url('app_cookies') }}{% endblock %}

{% block metaTags %}
    {% include '@CapcoApp/Default/meta.html.twig' with {
        'title': ('cookies'|trans({}, 'CapcoAppBundle')),
        'url':   url('app_cookies')
    } only %}
{% endblock %}

{% block title %}
    {{ parent() }} - {{ 'cookies' | trans({}, 'CapcoAppBundle') }}
{% endblock %}

{% block jumbotron %}
    <div class="jumbotron  jumbotron--custom  jumbotron--bg-1  text-center">
        <div class="container">
            <h1>{{ 'cookies' | trans({}, 'CapcoAppBundle') }}</h1>
        </div>
    </div>
{% endblock %}

{% block body %}
    {% set analytic_js = capco_site_parameter_value('snalytical-tracking-scripts-on-all-pages') %}
    {% set ads_js = capco_site_parameter_value('ad-scripts-on-all-pages') %}
    <section class="section--custom">
        <div class="container" style="max-width: 660px">
            {{ react_render_component('CookieContentApp') }}
        </div>
    </section>
{% endblock %}

<section id="header" class="section--header section--custom old-editor">
    <div class="container">
        {% if null != section.title %}
        <h2 class="h2">{{ section.title }}</h2>
        {% endif %}
        {% if null != section.teaser %}
            <p>{{ section.teaser }}</p>
        {% endif %}
        {% if null != section.body %}
            {{ section.body | raw }}
        {% endif %}
        <div class="section-clear"></div>
    </div>
</section>
